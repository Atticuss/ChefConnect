{
    "schema": "schema.graphql",
    "documents": "**/*.graphql",
    "extensions": {
        "strawberryShake": {
            "name": "ConferenceClient",
            "namespace": "Demo.GraphQL",
            "url": "https://workshop.chillicream.com/graphql/",
            "dependencyInjection": true,
            "strictSchemaValidation": true,
            "hashAlgorithm": "md5",
            "useSingleFile": true,
            "requestStrategy": "Default",
            "outputDirectoryName": "Generated",
            "noStore": false,
            "emitGeneratedCode": true,
            "razorComponents": false,
            "records": {
                "inputs": false,
                "entities": false
            },
            "transportProfiles": [{
                "default": "Http",
                "subscription": "WebSocket"
            }]
        }
    }
}